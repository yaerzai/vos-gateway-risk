        <dependency>
            <groupId>com.ytl</groupId>
            <artifactId>vos-common-lib-jms</artifactId>
            <version>1.0.4-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>com.ytl</groupId>
                    <artifactId>vos-common-lib-base</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.ytl</groupId>
            <artifactId>vos-jms-code</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>

        <dependency>
            <groupId>com.ytl</groupId>
            <artifactId>vos-common-lib-redis</artifactId>
            <version>1.0.0-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>com.ytl</groupId>
                    <artifactId>vos-common-lib-base</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.ytl</groupId>
            <artifactId>vos-common-lib-db</artifactId>
            <version>1.0.0-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>com.ytl</groupId>
