        <dependency>
            <groupId>com.ytl</groupId>
            <artifactId>vos-common-lib-swagger</artifactId>
            <version>1.0.1-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>com.ytl</groupId>
                    <artifactId>vos-common-lib-base</artifactId>
                </exclusion>
